{
  "name": "expo-router-starter",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npx expo start --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "check.types": "tsc",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.json",
    "lint.fix": "eslint . --ext .ts,.tsx,.js,.jsx,.json --fix",
    "format": "prettier --check .",
    "format.fix": "prettier --write .",
    "codegen.rm": "rm -f types/gql/*",
    "codegen.generate": "graphql-codegen",
    "codegen.watch": "graphql-codegen --watch",
    "codegen": "npm-run-all codegen.rm codegen.generate"
  },
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@types/react": "~18.2.14",
    "apollo-link-http": "^1.5.17",
    "expo": "^49.0.5",
    "expo-constants": "~14.4.2",
    "expo-device": "~5.4.0",
    "expo-font": "~11.4.0",
    "expo-linking": "~5.0.2",
    "expo-localization": "~14.3.0",
    "expo-notifications": "~0.20.1",
    "expo-router": "2.0.0",
    "expo-secure-store": "~12.3.1",
    "expo-splash-screen": "^0.20.4",
    "expo-status-bar": "~1.6.0",
    "i18n-js": "4.2.0",
    "lottie-react-native": "5.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.3",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-paper": "^5.8.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-web": "~0.19.6",
    "styled-components": "5.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/client-preset": "^4.0.0",
    "@types/graphql": "^14.5.0",
    "@types/react-native": "^0.72.2",
    "@types/styled-components-react-native": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "private": true
}
