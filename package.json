{
  "name": "expo-router-starter",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "check.types": "tsc",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.json",
    "lint.fix": "eslint . --ext .ts,.tsx,.js,.jsx,.json --fix",
    "format": "prettier --check .",
    "format.fix": "prettier --write .",
    "codegen.rm": "rm -f types/gql/*",
    "codegen.generate": "graphql-codegen",
    "codegen.watch": "graphql-codegen --watch",
    "codegen": "npm-run-all codegen.rm codegen.generate"
  },
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@types/react": "~18.0.27",
    "apollo-link-http": "^1.5.17",
    "expo": "~48.0.18",
    "expo-constants": "~14.2.1",
    "expo-device": "~5.2.1",
    "expo-font": "~11.1.1",
    "expo-linking": "~4.0.1",
    "expo-localization": "~14.1.1",
    "expo-notifications": "~0.18.1",
    "expo-router": "^1.5.3",
    "expo-secure-store": "~12.1.1",
    "expo-status-bar": "~1.4.4",
    "i18n-js": "^4.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.8",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-web": "~0.18.10",
    "styled-components": "^6.0.0-rc.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/client-preset": "^4.0.0",
    "@types/graphql": "^14.5.0",
    "@types/react-native": "^0.72.2",
    "@types/styled-components-react-native": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "typescript": "^4.9.4"
  },
  "resolutions": {
    "metro": "0.76.0",
    "metro-resolver": "0.76.0"
  },
  "private": true
}
